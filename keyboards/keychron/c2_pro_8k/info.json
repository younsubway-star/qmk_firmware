{
    "keyboard_name": "Keychron C2 Pro 8K",
    "manufacturer": "Keychron",
    "url": "https://github.com/Keychron",
    "maintainer": "Keychron",
    "processor": "AT32F405",
    "bootloader": "at32-dfu",
    "usb": {
        "vid": "0x3434"
    },
    "features": {
        "bootmagic": true,
        "dip_switch": true,
        "extrakey": true,
        "mousekey": true,
        "nkro": true,
        "rgb_matrix": true
    },
    "matrix_pins": {
        "cols": ["C7", "C8", "A14", "A15", "C10", "C11", "C13", "C14", "C15", "C0", "C1", "C2", "C3", "A0", "A1", "A2", "A3", "A12", "A11"],
        "rows": ["C12", "D2", "B3", "B4", "B5", "B6"]
    },
    "diode_direction": "COL2ROW",
    "dip_switch": {
        "pins": ["B1"]
    },
    "indicators": {
        "caps_lock": "B10",
        "num_lock":"C6",
        "on_state":1
    },
    "build": {
        "debounce_type": "custom"
    },
    "debounce": 30,
    "eeprom": {
        "wear_leveling": {
            "driver": "embedded_flash",
            "logical_size": 3072,
            "backing_size": 6144
        }
    },
    "rgb_matrix": {
        "driver": "snled27351_spi",
        "sleep":true,
        "animations": {
            "breathing": true,
            "band_spiral_val": true,
            "cycle_all": true,
            "cycle_left_right": true,
            "cycle_up_down": true,
            "rainbow_moving_chevron": true,
            "cycle_out_in": true,
            "cycle_out_in_dual": true,
            "cycle_pinwheel": true,
            "cycle_spiral": true,
            "dual_beacon": true,
            "rainbow_beacon": true,
            "jellybean_raindrops": true,
            "pixel_rain": true,
            "typing_heatmap": true,
            "digital_rain": true,
            "solid_reactive_simple": true,
            "solid_reactive_multiwide": true,
            "solid_reactive_multinexus": true,
            "splash": true,
            "solid_splash": true
        }
    }
}
